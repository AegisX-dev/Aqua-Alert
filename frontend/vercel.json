    {
    "version": 2,
    "name": "aqua-alert-frontend",
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ],
    "env": {
        "VITE_API_URL": "@aqua_alert_api_url"
    },
    "functions": {
        "app/api/health.js": {
        "maxDuration": 10
        }
    },
    "rewrites": [
        {
        "source": "/api/(.*)",
        "destination": "https://your-backend-url.vercel.app/api/$1"
        }
    ],
    "headers": [
        {
        "source": "/assets/(.*)",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
            }
        ]
        }
    ]
    }