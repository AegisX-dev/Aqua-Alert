    {
    "version": 2,
    "name": "aqua-alert-frontend",
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ],
    "env": {
        "VITE_API_URL": "@aqua_alert_api_url"
    }
    }